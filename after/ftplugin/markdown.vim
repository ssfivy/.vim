" Open first level fold for markdown files
"let g:vim_markdown_initial_foldlevel=10
let g:vim_markdown_folding_disabled=1

" Disable custom markdown key mappings for now
let g:markdown_no_default_key_mappings=1

" I like 4 spaces wide tab
setlocal expandtab
setlocal shiftwidth=4
setlocal softtabstop=4
